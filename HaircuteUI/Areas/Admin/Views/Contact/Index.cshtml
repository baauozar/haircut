@model IEnumerable<HaircuteUI.ViewModel.ContactViewModel>

<div class="table-responsive">
    <table class="table table-responsive-md">
        <thead>
            <tr>
             
                <th><strong>ROLL NO.</strong></th>
                <th><strong>Namen</strong></th>
                <th><strong>Last Name</strong></th>
                <th><strong>Phone Number</strong></th>
                <th><strong>Email</strong></th>
                <th><strong>Message</strong></th>
          
                <th><strong>Status</strong></th>
                <th><strong>Action</strong></th>

                <th><strong></strong></th>
            </tr>
        </thead>
        @{
            int number = 0;
        }
        @foreach (var x in Model)

        {
            <tbody>
                <tr>
                 
                    <td>
                        <strong>
                            @{
                                number++;
                            }@number
                        </strong>
                    </td>



                    <td>@x.Name</td>
                    <td>@x.LastName</td>
                    <td>@x.phonenumber</td>
                    <td>@x.Email</td>
                    <td>@x.Message</td>

                    <td>
                        <div class="d-flex align-items-center">
                            <i class="fa fa-circle text-success me-1"></i> @if (!x.IsDeleted)
                            {
                                <span>Not Deleted</span>
                            }
                        </div>
                    </td>
                    <td>
                        <div class="d-flex">
                            <button data-id="@x.Id" class="btn btn-primary shadow btn-xs sharp me-1 btnEdit" data-bs-toggle="modal" data-bs-target=".bd-example-modal-lg-edit">
                                <i class="fa fa-pencil"></i>
                            </button>
                            <button data-id="@x.Id" class="btn btn-danger shadow btn-xs sharp DeleteFaq"><i class="fa fa-trash"></i></button>
                        </div>
                    </td>
                </tr>



            </tbody>
        }
    </table>


</div>
<script src="~/lib/jquery/dist/jquery.min.js"></script>
<script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
<link rel="stylesheet" type="text/css" href="https://cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/toastr.min.css">
<script src="https://cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/toastr.min.js"></script>
<script src="
https://cdn.jsdelivr.net/npm/sweetalert2@11.7.28/dist/sweetalert2.all.min.js
"></script>
<link href="
https://cdn.jsdelivr.net/npm/sweetalert2@11.7.28/dist/sweetalert2.min.css
" rel="stylesheet">

@if (TempData["NotificationMessage"] != null)
{
    <script>
        toastr.success('@TempData["NotificationMessage"]');
    </script>
}

<script src="~/js/ajaxtest.js"></script>